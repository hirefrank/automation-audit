{
    "name": "automation-audit",
    "version": "0.0.1",
    "type": "module",
    "description": "This is an example of a Crawlee project.",
    "dependencies": {
        "apify": "^3.1.4",
        "crawlee": "^3.0.0",
        "playwright": "*"
    },
    "devDependencies": {
        "@apify/tsconfig": "^0.1.0",
        "@types/node": "^18.0.0",
        "ts-node": "^10.8.0",
        "typescript": "^5.0.0"
    },
    "scripts": {
        "start": "npm run start:dev",
        "start:prod": "node dist/main.js",
        "start:dev": "ts-node-esm -T src/main.ts",
        "build": "tsc",
        "assemble:services": "source .venv/bin/activate && csvs-to-sqlite storage/key_value_stores/default/services.csv audit.db -f name -f description",
        "assemble:templates": "source .venv/bin/activate && csvs-to-sqlite storage/key_value_stores/default/templates.csv audit.db -f title -f description",
        "assemble:connectors": "source .venv/bin/activate && csvs-to-sqlite storage/key_value_stores/default/connectors.csv audit.db -f name -f description -f title",
        "publish": "datasette publish vercel audit.db --project automation-audit",
        "test": "echo \"Error: oops, the actor has no tests yet, sad!\" && exit 1"
    },
    "author": "It's not you it's me",
    "license": "ISC"
}
